{
  "manifest_version": 2,

  "name": "CGCookie ReplyChecker",
  "description": "fFind questions without answers",
  "icons": { "128": "icon.png" },
  "version": "0.2",

  "browser_action": {
    "default_title": "CGC ReplyChecker",
    "default_icon": "icon.png",
    "default_popup": "layout.html"
  },
  "options_page": "options.html",
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "permissions": [
    "activeTab"
  ]
}
